import random 
def main_code():
    min = 1
    max = 6
    answer = input("Would you like to roll the dice?")
    if answer == 'yes' or answer == 'Yes':
        print(random.randint(min,max))
        print(random.randint(min,max))
    if answer == 'no' or answer == 'No':
        print("Then go away.")

main_code()
